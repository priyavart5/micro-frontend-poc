{
    "version": 2,
    "builds": [
      {
        "src": "host/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "host/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/host/$1"
      }
    ],
    "env": {
      "REACT_APP_CHAT_HOST": "http://localhost:3001",
      "REACT_APP_EMAIL_HOST": "http://localhost:3002"
    }
  }
  